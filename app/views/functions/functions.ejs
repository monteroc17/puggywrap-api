<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>

            <h1>Functions</h1>
            <div class="searchbar">
                <input type="search" name="searchbar" id="sb">
            </div>

            <div class="grid">
                <% functions.forEach(element => { %>
                    <div class="card" style="padding: 5px">
                        <h2><%= element.name %></h2>
                        <p><%= element.id %></p>
                    </div>
                <% }); %>
            </div>


            <%- include('../includes/end.ejs') %>