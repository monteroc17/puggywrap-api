<%- include('../includes/head.ejs') %>

    <link rel="stylesheet" href="/stylesheets/getting_started.css">
</head>
<body>
    <%- include('../includes/navigation.ejs') %>

        <div class="card" style="padding: 5px">
            <h2>Name</h2>
            <p> <%= functionDetails.name %> </p>
            <hr>
            <h3>Description</h3>
            <p> <%= functionDetails.description %> </p>
            <hr>
            <h3>Author</h3>
            <p> <%= functionDetails.userId %> </p>
            <hr>
            <h3>Code</h3>
            <p>  <%= version.function_code %></p>
        </div>
        <br>
        <div class="grid">
            <a class="btn" href="/admin/functions">Back</a>
            <a class="btn" href="/admin/edit_function/<%= functionDetails.id %>">Edit</a>
            <button class="btn" type="submit">Export</button>
        </div>
        
<%- include('../includes/end.ejs') %>